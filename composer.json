{
    "name": "jotaelesalinas/create-skeleton",
    "type": "composer-plugin",
    "description": "Composer plugin to create a PHP package skeleton",
    "keywords": [
        "jotaelesalinas",
        "create-skeleton",
        "thephpleague",
        "skeleton",
        "create",
        "composer",
        "package",
        "packagist"
    ],
    "homepage": "https://github.com/jotaelesalinas/create-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "JosÃ© Luis Salinas",
            "email": "jotaelesalinas@example.com",
            "homepage": "https://github.com/jotaelesalinas",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "JLSalinas\\Composer\\CreateSkeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JLSalinas\\Composer\\CreateSkeleton\\Tests": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 --no-patch src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "class": "JLSalinas\\Composer\\CreateSkeleton"
    },
    "config": {
        "sort-packages": true
    }
}
