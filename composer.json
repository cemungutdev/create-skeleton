{
    "name": "jotaelesalinas/create-skeleton",
    "type": "composer-plugin",
    "description": "Composer plugin to create a PHP package skeleton",
    "keywords": [
        "jotaelesalinas",
        "create-skeleton"
    ],
    "homepage": "https://github.com/jotaelesalinas/create-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Jos‚ Luis Salinas",
            "email": "jotaelesalinas@example.com",
            "homepage": "https://github.com/jotaelesalinas",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "JLSalinas\\CreateSkeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JLSalinas\\CreateSkeleton\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "class": "JLSalinas\\SkeletonPrefiller"
    },
    "config": {
        "sort-packages": true
    }
}
